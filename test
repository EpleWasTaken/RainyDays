const menContainer = document.querySelector(".menResultsContainer");

const menCategoryURL = "http://rainydays.svanevik.one/wp-json/wc/store/products?category=16";

// const proxy = "https://noroffcors.herokuapp.com/";

// const corsFix = proxy + menCategoryURL;

// console.log(corsFix);

// console.log(menCategoryURL);

async function fetchMenProducts() {
    try {
        const productsResult = await fetch(menCategoryURL);
        const productsJson = await productsResult.json();
        console.log(productsJson);

        const products = productsJson;

        products.forEach(function (product) {
            menContainer.innerHTML += `<div class="productsCard">
                                        <a href="html/productDetails.html?id=${product.id}&${product.name}">
                                        <div class="details-image" style="background-image: url('${product.images[0].thumbnail}')">
                                        </a>
                                        </div>`;
        });
    } catch {
        console.log("Not working");
    }
}

fetchMenProducts();









